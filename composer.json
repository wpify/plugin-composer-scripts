{
  "name": "wpify/plugin-composer-scripts",
  "description": "Composer plugin with scripts for WPify Plugins",
  "type": "composer-plugin",
  "require": {
	"php": "^7.3 || ^8.0",
	"composer-plugin-api": "^1.1 || ^2.0"
  },
  "require-dev": {
	"composer/composer": "^1.10 || ^2.0"
  },
  "license": "GPL-2.0-or-later",
  "autoload": {
	"psr-4": {
	  "Wpify\\PluginComposerScripts\\": "src/"
	}
  },
  "config": {
	"platform": {
	  "php": "7.3"
	},
	"preferred-install": "dist",
	"sort-packages": true
  },
  "authors": [
	{
	  "name": "WPify",
	  "email": "info@wpify.io"
	}
  ],
  "extra": {
	"class": "Wpify\\PluginComposerScripts\\Plugin"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
